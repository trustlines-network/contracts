{
  "extends": "solium:all",
  "plugins": ["security"],
  "rules": {
    "indentation": ["error", 4],
    "function-order": "error",
    "mixedcase": "error",
    "quotes": ["error", "double"],
    "error-reason": "error",
    "security/no-inline-assembly": "off",
    "security/no-block-members": "off",
    "operator-whitespace": "off"
  }
}
